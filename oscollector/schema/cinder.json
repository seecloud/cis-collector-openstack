{
    "volumes":{
        "$schema":"http://json-schema.org/draft-04/schema#",
        "name":"volume",
        "type":"object",
        "parent":"cinder",
        "root_id":"id",
        "properties":{
            "availability_zone":{
                "type":"string"
            },
            "bootable":{
                "type":"boolean"
            },
            "created_at":{
                "type":"string"
            },
            "display_description":{
                "type":"string"
            },
            "encrypted":{
                "type":"boolean"
            },
            "metadata":{
                "type":"object",
                "properties":{
                    "attached_mode":{
                        "type":"string"
                    },
                    "readonly":{
                        "type":"string"
                    }
                }
            },
            "multiattach":{
                "type":"boolean"
            },
            "os-vol-host-attr:host":{
                "type":"string"
            },
            "os-vol-mig-status-attr:migstat":{
                "type":"string"
            },
            "os-vol-mig-status-attr:name_id":{
                "type":"string"
            },
            "os-vol-tenant-attr:tenant_id":{
                "type":"string"
            },
            "size":{
                "type":"integer"
            },
            "snapshot_id":{
                "type":"string"
            },
            "source_volid":{
                "type":"string"
            },
            "status":{
                "type":"string"
            },
            "volume_image_metadata":{
                "type":"object",
                "properties":{
                    "checksum":{
                        "type":"string"
                    },
                    "container_format":{
                        "type":"string"
                    },
                    "disk_format":{
                        "type":"string"
                    },
                    "image_id":{
                        "type":"string"
                    },
                    "image_name":{
                        "type":"string"
                    },
                    "kernel_id":{
                        "type":"string"
                    },
                    "min_disk":{
                        "type":"integer"
                    },
                    "min_ram":{
                        "type":"integer"
                    },
                    "ramdisk_id":{
                        "type":"string"
                    },
                    "size":{
                        "type":"integer"
                    }
                }
            },
            "volume_type":{
                "type":"string"
            },
            "attachments":{
                "type":"array",
              "items":{
                "type":"object",
                "properties":{
                  "attachment_id":{
                    "type":"string"
                  },
                  "device":{
                    "type":"string"
                  },
                  "host_name":{
                    "type":"string"
                  },
                  "id":{
                    "type":"string"
                  },
                  "server_id":{
                    "type":"string"
                  },
                  "volume_id":{
                    "type":"string"
                  }
                }
              }
            }
        }
    },
    "volume_snapshots":{
        "$schema":"http://json-schema.org/draft-04/schema#",
        "name":"volume_snapshot",
        "type":"object",
        "parent":"keystone",
        "root_id":"id",
        "properties":{
            "created_at":{
                "type":"string"
            },
            "display_description":{
                "type":"string"
            },
            "metadata":{
                "type":"object"
            },
            "os-extended-snapshot-attributes:progress":{
                "type":"string"
            },
            "os-extended-snapshot-attributes:project_id":{
                "type":"string"
            },
            "size":{
                "type":"integer"
            },
            "status":{
                "type":"string"
            },
            "volume_id":{
                "type":"string"
            }
        }
    }
}