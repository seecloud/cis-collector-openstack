{
    "images":{
        "$schema":"http://json-schema.org/draft-04/schema#",
        "name":"image",
        "type":"object",
        "parent":"nova",
        "root_id":"id",
        "properties":{
            "name":{
                "type":"string"
            },
            "created":{
                "type":"string"
            },
            "metadata":{
                "type":"object",
                "properties": {
                    "kernel_id":{
                        "type":"string"
                    },
                    "ramdisk_id":{
                        "type":"string"
                    }
                }
            },
            "updated":{
                "type":"string"
            },
            "status":{
                "type":"string"
            },
            "progress":{
                "type":"integer"
            },
            "minDisk":{
                "type":"integer"
            },
            "minRam":{
                "type":"integer"
            }
        }
    },
    "flavors":{
        "$schema":"http://json-schema.org/draft-04/schema#",
        "name":"flavor",
        "type":"object",
        "parent":"nova",
        "root_id":"id",
        "properties":{
            "name":{
                "type":"string"
            },
            "ram":{
                "type":"integer"
            },
            "vcpus":{
                "type":"integer"
            },
            "disk":{
                "type":"integer"
            },
            "OS-FLV-DISABLED:disabled":{
                "type":"boolean"
            },
            "OS-FLV-EXT-DATA:ephemeral":{
                "type":"integer"
            },
            "os-flavor-access:is_public":{
                "type":"boolean"
            },
            "swap":{
                "type":"string"
            }
        }
    },
    "keypairs":{
        "$schema":"http://json-schema.org/draft-04/schema#",
        "name":"keypair",
        "type":"object",
        "parent":"nova",
        "root_id":"name",
        "root":"keypair",
        "properties":{
            "fingerprint":{
                "type":"string"
            },
            "public_key":{
                "type":"string"
            }
        }
    },
    "hypervisors":{
        "$schema":"http://json-schema.org/draft-04/schema#",
        "name":"hypervisor",
        "type":"object",
        "parent":"nova",
        "root_id":"id",
        "properties":{
            "cpu_info":{
                "type":"string"
            },
            "hypervisor_hostname":{
                "type":"string"
            },
            "hypervisor_type":{
                "type":"string"
            },
            "local_gb":{
                "type":"integer"
            },
            "local_gb_used":{
                "type":"integer"
            },
            "memory_mb":{
                "type":"integer"
            },
            "memory_mb_used":{
                "type":"integer"
            },
            "free_disk_gb":{
                "type":"integer"
            },
            "free_ram_mb":{
                "type":"integer"
            },
            "disk_available_least":{
                "type":"integer"
            },
            "running_vms":{
                "type":"integer"
            },
            "state":{
                "type":"string"
            },
            "status":{
                "type":"string"
            },
            "service":{
                "type":"object",
                "properties":{
                    "disabled_reason":{
                        "type":"string"
                    },
                    "host":{
                        "type":"string"
                    },
                    "id":{
                        "type":"integer"
                    }
                }
            },
            "host_ip":{
                "type":"string"
            },
            "vcpus":{
                "type":"integer"
            },
            "vcpus_used":{
                "type":"integer"
            },
            "current_workload":{
                "type":"integer"
            }
        },
        "required":[
            "hypervisor_hostname"
        ]
    },
    "quotas":{
        "$schema":"http://json-schema.org/draft-04/schema#",
        "name":"quota",
        "type":"object",
        "parent":"nova",
        "root_id":"id",
        "properties":{
            "cores":{
                "type":"integer"
            },
            "fixed_ips":{
                "type":"integer"
            },
            "floating_ips":{
                "type":"integer"
            },
            "injected_file_content_bytes":{
                "type":"integer"
            },
            "injected_file_path_bytes":{
                "type":"integer"
            },
            "injected_files":{
                "type":"integer"
            },
            "instances":{
                "type":"integer"
            },
            "key_pairs":{
                "type":"integer"
            },
            "metadata_items":{
                "type":"integer"
            },
            "ram":{
                "type":"integer"
            },
            "security_group_rules":{
                "type":"integer"
            },
            "security_groups":{
                "type":"integer"
            },
            "server_group_members":{
                "type":"integer"
            },
            "server_groups":{
                "type":"integer"
            }
        }
    },
    "servers":{
        "$schema":"http://json-schema.org/draft-04/schema#",
        "name":"server",
        "type":"object",
        "parent":"nova",
        "root_id":"id",
        "properties":{
            "name":{
                "type":"string"
            },
            "hostId":{
                "type":"string"
            },
            "image":{
                "type":"string"
            },
            "security_groups":{
                "type":"array"
            },
            "os-extended-volumes:volumes_attached":{
                "type":"array",
                "items":{
                    "type":"object",
                    "properties":{
                        "id":{
                            "type":"string"
                        }
                    }
                }
            },
            "OS-EXT-AZ:availability_zone":{
                "type":"string"
            },
            "OS-EXT-SRV-ATTR:hypervisor_hostname":{
                "type":"string"
            },
            "OS-EXT-SRV-ATTR:host":{
                "type":"string"
            },
            "OS-EXT-SRV-ATTR:instance_name":{
                "type":"string"
            },
            "OS-EXT-STS:power_state":{
                "type":"integer"
            },
            "OS-EXT-STS:vm_state":{
                "type":"string"
            },
            "OS-SRV-USG:launched_at":{
                "type":"string"
            },
            "OS-SRV-USG:terminated_at":{
                "type":"string"
            },
            "addresses":{
                "type":"object"
            },
            "created":{
                "type":"string"
            },
            "flavor":{
                "type":"object"
            },
            "key_name":{
                "type":"string"
            },
            "tenant_id":{
                "type":"string"
            },
            "updated":{
                "type":"string"
            },
            "user_id":{
                "type":"string"
            }
        },
        "required":[
            "name"
        ]
    }
}