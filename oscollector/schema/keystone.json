        "parent":"keystone",
        "root_id":"id",
        "properties":{
            "name":{
                "type":"string"
            },
            "description":{
                "type":"string"
            },
            "enabled":{
                "type":"boolean"
            }
        },
        "required":[
            "name"
        ]
    },
    "users":{
        "$schema":"http://json-schema.org/draft-04/schema#",
